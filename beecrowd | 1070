#include <stdio.h>
int main() {
    int n, i;
    scanf("%d", &n);
     i=n;
    while (i<=n+11) {
        if (i % 2 != 0) {
            printf("%d\n", i);
        }
      i++;
     }
    return 0;
}
//another way
#include <stdio.h>
int main() {
    int n, i,cnt;
    scanf("%d", &n);
    cnt=1;
    i=n;
    while (cnt <=6) {
        if (i % 2 != 0) {
            printf("%d\n", i);
            cnt++;
        }
       i++;
     }
    return 0;
}
